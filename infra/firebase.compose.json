{
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  },
  "functions": {
    "predeploy": ["yarn build:functions"],
    "source": "functions",
    "runtime": "nodejs18"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "auth": {
      "port": 9099,
      "host": "0.0.0.0"
    },
    "functions": {
      "port": 5001,
      "host": "0.0.0.0"
    },
    "firestore": {
      "port": 8080,
      "host": "0.0.0.0"
    },
    "hosting": {
      "port": 5000,
      "host": "0.0.0.0"
    },
    "ui": {
      "enabled": true,
      "port": 3010,
      "host": "0.0.0.0"
    },
    "pubsub": {
      "port": 8085,
      "host": "0.0.0.0"
    },
    "storage": {
      "port": 9199,
      "host": "0.0.0.0"
    },
    "hub": {
      "port": "4400",
      "host": "0.0.0.0"
    },
    "eventarc": {
      "port": "9299",
      "host": "0.0.0.0"
    },
    "logging": {
      "port": "4500",
      "host": "0.0.0.0"
    }
  },
  "storage": {
    "rules": "storage.rules"
  },
  "extensions": {
    "firestore-send-email": "firebase/firestore-send-email@0.1.26"
  }
}
